/*index.wxss*/
.sTab{
    width: 100%;
    border-bottom: 2rpx solid #ccc;
    text-align: center;
    height: 88rpx;
    line-height: 88rpx;
    display: flex;
    flex-flow: row;
    justify-content: space-between;
}
.sTabItem{
    width: 25%; 
    color:#434343;
}
.on{
    color:#F65959;
    border-bottom: 4rpx solid #F65959;
}
.swiper-wrap{
  /* text-align: center; */
  height: 1000rpx;
}
.swiper {
  width: 100%;
}
.swiper image {
  height: 100%;
  width: 100%;
}
/* 特效库 */
.animation-element-wrapper {
  display: flex;
  width: 100%;
  padding-top: 150rpx;
  padding-bottom: 150rpx;
  justify-content: center;
  overflow: hidden;
  background-color: #ffffff;
}
.animation-element {
  width: 200rpx;
  height: 200rpx;
  background-color: #1AAD19;
}
.animation-buttons {
  padding: 30rpx 30rpx 10rpx;
  width: 100%;
  height: 600rpx;
  box-sizing: border-box;
}
.animation-button {
  float: left;
  line-height: 2;
  /* width: 300rpx; */
  margin: 15rpx 12rpx;
}

.animation-button-reset {
  width: 620rpx;
}
/* 表单提交 */
#adduser{  
    /* background: #f5f5f5;   */
    margin:50rpx auto;
}  
.section__title{  
    float: left;  
    width:30%;  
}  
.form-group{  
    float: right;  
    width: 70%;  
}  
.section{  
    clear: both;  
    width:90%;  
    margin: 0 auto;  
    height: 100rpx;
}  
.input-text{  
	line-height: 30rpx;
    border: 1px solid #ddd;  
        padding: 5px;  
}  
.button{  
    border: 1px solid #1aad19;  
    border-radius: 2px;  
}  
.picker{  
    padding: 5px;  
    background-color: #FFFFFF;  
} 
